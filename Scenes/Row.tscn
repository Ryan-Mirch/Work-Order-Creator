script/source = "extends ColorRect

# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"
export var ID = 0
onready var Short = $\"HBoxContainer/Box5/ConfirmationDialog/Grid/HBoxContainer/Short\"
onready var Tall = $\"HBoxContainer/Box5/ConfirmationDialog/Grid/HBoxContainer/Tall\"
# Called when the node enters the scene tree for the first time.
func _ready():
	if ID == 0:
		Short.pressed = true
	if ID == 1:
		Tall.pressed = true
		$\"AnimationPlayer\".play(\"Row\")
		
	

# Called every frame. 'delta' is the elapsed time since the previous frame.
#func _process(delta):
#	pass


func _on_Tall_pressed():
	if ID == 0:
		$\"AnimationPlayer\".play(\"Row\")
		ID = 1

func _on_Short_pressed():
	if ID == 1:
		$\"AnimationPlayer\".play_backwards(\"Row\")
		ID = 0
"

