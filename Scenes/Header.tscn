func _on_LineEdit_text_changed(new_text):
	if new_text == \"\":
		text = \"Page name can not be blank\"
		return
	
	for pageInstance in get_tree().get_nodes_in_group(\"Page Instance\"):
		if pageInstance.name == new_text:
			text = \"Page name must be unique\"
			return
	
	text = \"\"
	
"

