onready var Short = $\"HBoxContainer/Box5/ConfirmationDialog/VBox/Row1/ShortTall/Short\"
onready var Tall = $\"HBoxContainer/Box5/ConfirmationDialog/VBox/Row1/ShortTall/Tall\"
